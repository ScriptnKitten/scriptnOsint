#!/bin/bash
#OSINT via Facebook
#By: Jessica Ackerman @ScriptnKitten
#format: osintFacebook <facebookUserUrl>

rul=$1

#site
s1="url"

#_______________________________________________________________#
#Sites in Progress:

#_______________________________________________________________#
for i in 1
do
        tempString='s'$i
        fString+=$" -new-tab -url "
        fString+=${!tempString}
done

firefox -private $fString

